{
    "name": "andrey-helldar/yandex-goods-prices",
    "description": "Simple Yandex Markup Generator for Online Stores.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "goods",
        "goods-prices",
        "prices",
        "price",
        "yandex",
        "seo optimized",
        "seo",
        "shop",
        "e-shop"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://github.com/andrey-helldar"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/yandex-goods-prices/issues",
        "source": "https://github.com/andrey-helldar/yandex-goods-prices"
    },
    "require": {
        "php": ">=7.1.3 || ^8.0",
        "ext-dom": "*",
        "andrey-helldar/xml-core": "^1.0",
        "illuminate/validation": "^5.6 || ^6.0 || ^7.0 || ^8.0",
        "symfony/http-kernel": "^4.0 || ^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8 || ^4.0",
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.0"
    },
    "suggest": {
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Helldar\\Yandex\\GoodsPrices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "friendsofphp/php-cs-fixer": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\Yandex\\GoodsPrices\\ServiceProvider"
            ]
        }
    }
}