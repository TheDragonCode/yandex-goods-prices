{
    "name": "andrey-helldar/yandex-goods-prices",
    "description": "Simple Yandex Markup Generator for Online Stores.",
    "type": "library",
    "license": "MIT",
    "keywords": ["goods", "goods-prices", "prices", "price", "yandex", "seo optimized", "seo", "shop", "e-shop"],
    "support": {
        "issues": "https://github.com/andrey-helldar/yandex-goods-prices/issues",
        "source": "https://github.com/andrey-helldar/yandex-goods-prices"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-dom": "*",
        "andrey-helldar/xml-core": "^1.0",
        "illuminate/validation": "^5.6|^6.0",
        "symfony/http-kernel": "^4.0|^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "suggest": {
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Yandex\\GoodsPrices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\Yandex\\GoodsPrices\\ServiceProvider"
            ]
        }
    }
}
